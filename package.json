{
  "name": "iobroker.template_react",
  "version": "0.0.1",
  "description": "Template React with AlCalzone Hooks",
  "contributors": [
    {
      "name": "Issi",
      "email": "issi.dev.iobroker@gmail.com"
    }
  ],
  "homepage": "https://github.com/xXBJXx/ioBroker.template_react",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "template",
    "Smart Home",
    "home automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xXBJXx/ioBroker.template_react"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.8",
    "copy-to-clipboard": "^3.3.3",
    "i18n": "^0.15.1",
    "js-cookie": "^3.0.5"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.7.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.7.0",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@alcalzone/release-script-plugin-manual-review": "^3.7.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.13.0",
    "@iobroker/adapter-dev": "^1.3.0",
    "@iobroker/testing": "^4.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.15.11",
    "@mui/styles": "^5.13.2",
    "@tsconfig/node16": "^1.0.3",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/i18n": "^0.13.12",
    "@types/js-cookie": "^3.0.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.13.14",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.12",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "axios": "^1.6.7",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.4",
    "iobroker-react": "git+https://git@github.com/xXBJXx/iobroker-react.git#IssiVersion",
    "mocha": "^10.2.0",
    "prettier": "^2.8.4",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "rimraf": "^4.1.3",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build admin/build",
    "build": "build-adapter all",
    "watch": "build-adapter all --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit && tsc --noEmit -p admin/tsconfig.json",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/xXBJXx/ioBroker.template_react/issues"
  },
  "readmeFilename": "README.md"
}
