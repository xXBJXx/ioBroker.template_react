{
  "name": "iobroker.template_react",
  "version": "0.0.1",
  "description": "Template React with AlCalzone Hooks",
  "contributors": [{
	"name": "Issi",
	"email": "issi.dev.iobroker@gmail.com"
  }],
  "homepage": "https://github.com/xXBJXx/ioBroker.template_react",
  "license": "MIT",
  "keywords": [
	"ioBroker",
	"template",
	"Smart Home",
	"home automation"
  ],
  "repository": {
	"type": "git",
	"url": "https://github.com/xXBJXx/ioBroker.template_react"
  },
  "dependencies": {
	"@iobroker/adapter-core": "^2.6.0"
  },
  "devDependencies": {
	"@alcalzone/release-script": "^3.5.6",
	"@alcalzone/release-script-plugin-iobroker": "^3.5.7",
	"@alcalzone/release-script-plugin-license": "^3.5.3",
	"@emotion/react": "^11.8.2",
	"@emotion/styled": "^11.8.1",
	"@iobroker/adapter-react": "2.0.22",
	"@iobroker/adapter-dev": "^1.0.0",
	"@iobroker/dev-server": "github:ioBroker/dev-server",
	"@iobroker/socket-client": "^1.0.11",
	"@iobroker/testing": "^2.5.6",
	"@mui/icons-material": "^5.5.1",
	"@mui/material": "^5.5.3",
	"@mui/styles": "^5.5.3",
	"@types/chai": "^4.3.0",
	"@types/chai-as-promised": "^7.1.5",
	"@types/mocha": "^9.1.0",
	"@types/node": "^17.0.23",
	"@types/proxyquire": "^1.3.28",
	"@types/react": "^17.0.43",
	"@types/react-dom": "^17.0.14",
	"@types/sinon": "^10.0.11",
	"@types/sinon-chai": "^3.2.8",
	"@typescript-eslint/eslint-plugin": "^5.17.0",
	"@typescript-eslint/parser": "^5.17.0",
	"axios": "^0.26.1",
	"chai": "^4.3.6",
	"chai-as-promised": "^7.1.1",
	"eslint": "^8.12.0",
	"eslint-config-prettier": "^8.5.0",
	"eslint-plugin-prettier": "^4.0.0",
	"eslint-plugin-react": "^7.29.4",
	"iobroker-react": "github:AlCalzone/iobroker-react",
	"mocha": "^9.2.2",
	"nyc": "^15.1.0",
	"prettier": "^2.6.1",
	"proxyquire": "^2.1.3",
	"react": "^17.0.2",
	"react-dom": "^17.0.2",
	"rimraf": "^3.0.2",
	"sinon": "^13.0.1",
	"sinon-chai": "^3.7.0",
	"source-map-support": "^0.5.21",
	"tiny-glob": "^0.2.9",
	"ts-node": "^10.7.0",
	"typescript": "~4.6.3"
  },
  "main": "build/main.js",
  "files": [
	"admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
	"admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
	"admin/build/",
	"build/",
	"www/",
	"io-package.json",
	"LICENSE"
  ],
  "scripts": {
	"prebuild": "rimraf build admin/build",
	"build": "build-adapter all",
	"watch": "build-adapter all --watch",
	"prebuild:ts": "rimraf build",
	"build:ts": "build-adapter ts",
	"watch:ts": "build-adapter ts --watch",
	"prebuild:react": "rimraf admin/build",
	"build:react": "build-adapter react",
	"watch:react": "build-adapter react --watch",
	"test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
	"test:package": "mocha test/package --exit",
	"test:unit": "mocha test/unit --exit",
	"test:integration": "mocha test/integration --exit",
	"test": "npm run test:ts && npm run test:package",
	"check": "tsc --noEmit",
	"coverage": "nyc npm run test:ts",
	"lint": "eslint --ext .ts,.tsx src/ admin/src/",
	"translate": "translate-adapter",
	"release": "release-script"
  },
  "nyc": {
	"include": [
	  "src/**/*.ts"
	],
	"exclude": [
	  "src/**/*.test.ts"
	],
	"extension": [
	  ".ts"
	],
	"require": [
	  "ts-node/register"
	],
	"reporter": [
	  "text-summary",
	  "html"
	],
	"sourceMap": true,
	"instrument": true
  },
  "bugs": {
	"url": "https://github.com/xXBJXx/ioBroker.template_react/issues"
  },
  "readmeFilename": "README.md"
}
